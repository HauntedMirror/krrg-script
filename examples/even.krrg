// 引数未満の、正の偶数をすべて出力します。
krrg_mrh(krrg_sub) {
  krg_sub = !(krrgmrh - krrgmrh);
  krrrg_sub = krrgmrh - krrgmrh;
  krrrrg_sub = krg_sub;
  while (krrrrg_sub < krrg_sub) {
    if (krrrrg_sub % (krg_sub + krg_sub) == krrrg_sub) {
      #くるるぎはっぴょうかい(krrrrg_sub);
    }
    krrrrg_sub = krrrrg_sub + krg_sub;
  }
}
